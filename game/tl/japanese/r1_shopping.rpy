translate japanese r1_shopping_66171bf2:

    # p "You want to try something on?"
    p "「何かが着てみたい？」"

# game/r1_shopping.rpy:43
translate japanese r1_shopping_d207e06f:

    # s "Yes!"
    s "「うん！」"

# game/r1_shopping.rpy:45
